# ==========================================
# Parser Configuration
# ==========================================

# URL callback endpoint для получения логов от Python parser
PARSER_CALLBACK_URL=http://localhost:8000/api/internal/parser/callback

# Токен безопасности для callback'ов (используется для HMAC validation)
# Установите уникальное значение в production!
PARSER_CALLBACK_TOKEN=your-super-secret-token-change-in-production

# Путь к Python 3 интерпретатору
PARSER_PYTHON_PATH=python3

# Максимальный timeout для парсинга (в секундах)
PARSER_TIMEOUT=3600

# Размер буфера логов перед отправкой на сервер (количество логов)
PARSER_LOG_BUFFER_SIZE=10

# Очередь для выполнения парсинга (database, redis, sync)
PARSER_QUEUE=database
